<ListingElement Class="@Class" Style="@Style" Icon="@Icon">
    <MudText>@Value</MudText>
</ListingElement>

@code {

    [Parameter]
    public required string Icon { get; set; }

    [Parameter]
    public required string Value { get; set; }

    [Parameter]
    public string? Class { get; set; }

    [Parameter]
    public string? Style { get; set; }
}
